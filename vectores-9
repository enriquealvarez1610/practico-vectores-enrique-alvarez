//9.	Concatenar dos vectores de tama√±o n y m respectivamente. [a1,a2,......an;]  [b1,b2,....bm]; concatenado resulta [a1,a2,......an,b1,b2,....bm] con n+m elementos. con vectores
#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;
    cout << "Ingrese la cantidad de elementos del primer vector: ";
    cin >> n;
    cout << "Ingrese la cantidad de elementos del segundo vector: ";
    cin >> m;

    vector<int> A(n);
    vector<int> B(m);

    cout << "Ingrese los elementos del primer vector:" << endl;
    for (int i = 0; i < n; ++i) {
        cin >> A[i];
    }

    cout << "Ingrese los elementos del segundo vector:" << endl;
    for (int i = 0; i < m; ++i) {
        cin >> B[i];
    }

    vector<int> concatenado;
    concatenado.reserve(n + m);

    concatenado.insert(concatenado.end(), A.begin(), A.end());
    concatenado.insert(concatenado.end(), B.begin(), B.end());

    cout << "Vector concatenado: ";
    for (int val : concatenado) {
        cout << val << " ";
    }
    cout << endl;

    return 0;
}
