// Hacer un programa para eliminar los  n√∫meros duplicados de un vector. con vectores
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
int main() {
    int n;
    cout << "Ingrese la cantidad de elementos del vector: ";
    cin >> n;

    vector<int> numeros(n);
    cout << "Ingrese los elementos del vector:" << endl;
    for (int i = 0; i < n; ++i) {
        cin >> numeros[i];
    }

    // Ordenar el vector
    sort(numeros.begin(), numeros.end());

    // Eliminar duplicados
    auto last = unique(numeros.begin(), numeros.end());
    numeros.erase(last, numeros.end());

    cout << "Vector sin duplicados:" << endl;
    for (int num : numeros) {
        cout << num << " ";
    }
    cout << endl;

    return 0;
}
