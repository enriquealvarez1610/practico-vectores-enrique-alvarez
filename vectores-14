//Hacer un programa para clasificar una lista de valores (entero o realies) en orden ascendente. con vectores
#include <iostream> 
#include <vector>
#include <algorithm>
using namespace std;
int main() {
    int n;
    cout << "Ingrese la cantidad de valores: ";
    cin >> n;

    vector<float> valores(n);
    cout << "Ingrese los valores:" << endl;
    for (int i = 0; i < n; ++i) {
        cin >> valores[i];
    }

    sort(valores.begin(), valores.end());

    cout << "Valores en orden ascendente:" << endl;
    for (const float& val : valores) {
        cout << val << " ";
    }
    cout << endl;

    return 0;
}
