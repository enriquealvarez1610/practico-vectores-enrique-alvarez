#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;

    cout << "Ingrese la cantidad de valores del vector: ";
    cin >> n;

    vector<double> v(n);

    cout << "Ingrese los " << n << " valores:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> v[i];
    }

    double maximo = v[0];

    for (int i = 1; i < n; i++) {
        if (v[i] > maximo) {
            maximo = v[i];
        }
    }

    cout << "\nEl valor maximo del vector es: " << maximo << endl;

    return 0;
}
